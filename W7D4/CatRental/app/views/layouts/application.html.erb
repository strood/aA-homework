    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <link href='https://fonts.googleapis.com/css?family=Sofia' rel='stylesheet'>
  </head>

  <body>
    <header class="main-header">
